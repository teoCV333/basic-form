<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="386dp"
        android:layout_height="535dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="17dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <EditText
            android:id="@+id/editTextVote"
            android:layout_width="240dp"
            android:layout_height="45dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="164dp"
            android:enabled="false"
            android:fontFamily="monospace"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextBirthRes"
            android:layout_width="240dp"
            android:layout_height="45dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="44dp"
            android:enabled="false"
            android:fontFamily="monospace"
            app:layout_constraintBottom_toTopOf="@+id/editTextVote"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextCCRes"
            android:layout_width="240dp"
            android:layout_height="45dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="44dp"
            android:enabled="false"
            android:fontFamily="monospace"
            app:layout_constraintBottom_toTopOf="@+id/editTextBirthRes"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextNameRes"
            android:layout_width="240dp"
            android:layout_height="45dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="316dp"
            android:enabled="false"
            android:fontFamily="monospace"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.375">

        </EditText>

        <TextView
            android:id="@+id/textViewVote"
            android:layout_width="123dp"
            android:layout_height="45dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="164dp"
            android:fontFamily="monospace"
            android:gravity="center|left"
            android:paddingLeft="18dp"
            android:text="@string/label_vote"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/editTextVote"
            app:layout_constraintHorizontal_bias="0.136"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewBirth"
            android:layout_width="123dp"
            android:layout_height="45dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="44dp"
            android:fontFamily="monospace"
            android:gravity="start|center_vertical"
            android:paddingLeft="18dp"
            android:text="@string/label_birth"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewVote"
            app:layout_constraintEnd_toStartOf="@+id/editTextBirthRes"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewCC"
            android:layout_width="123dp"
            android:layout_height="45dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="44dp"
            android:fontFamily="monospace"
            android:gravity="center|left"
            android:paddingLeft="18dp"
            android:text="@string/label_cc"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewBirth"
            app:layout_constraintEnd_toStartOf="@+id/editTextCCRes"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewName"
            android:layout_width="123dp"
            android:layout_height="45dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="40dp"
            android:fontFamily="monospace"
            android:gravity="center|left"
            android:paddingLeft="18dp"
            android:text="@string/label_name"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewCC"
            app:layout_constraintEnd_toStartOf="@+id/editTextNameRes"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.258" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="363dp"
            android:layout_height="105dp"
            android:layout_marginTop="412dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.521"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/buttonEdit"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:fontFamily="monospace"
                android:onClick="edit"
                android:text="@string/btn_edit"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/buttonConfirm"
                app:layout_constraintHorizontal_bias="0.592"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/buttonConfirm"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:fontFamily="monospace"
                android:onClick="confirm"
                android:text="@string/btn_confirm"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="342dp"
            android:layout_height="96dp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="14dp"
            android:fontFamily="monospace"
            android:gravity="center"
            android:text="@string/tittle_2"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/success"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/success" />
</androidx.constraintlayout.widget.ConstraintLayout>