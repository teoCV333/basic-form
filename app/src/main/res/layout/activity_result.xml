<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="300dp"
        android:layout_height="99dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <Button
            android:id="@+id/buttonEdit"
            android:layout_width="123dp"
            android:layout_height="59dp"
            android:onClick="edit"
            android:text="Editar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonConfirm"
            app:layout_constraintHorizontal_bias="0.421"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonConfirm"
            android:layout_width="123dp"
            android:layout_height="59dp"
            android:layout_marginEnd="16dp"
            android:onClick="confirm"
            android:text="Confirmar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="351dp"
        android:layout_height="374dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="17dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/editTextVote"
            android:layout_width="231dp"
            android:layout_height="44dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="88dp"
            android:enabled="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextBirthRes"
            android:layout_width="231dp"
            android:layout_height="44dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="20dp"
            android:enabled="false"
            app:layout_constraintBottom_toTopOf="@+id/editTextVote"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextCCRes"
            android:layout_width="231dp"
            android:layout_height="44dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="12dp"
            android:enabled="false"
            app:layout_constraintBottom_toTopOf="@+id/editTextBirthRes"
            app:layout_constraintEnd_toEndOf="parent">

        </EditText>

        <EditText
            android:id="@+id/editTextNameRes"
            android:layout_width="231dp"
            android:layout_height="44dp"
            android:layout_marginEnd="4dp"
            android:enabled="false"
            app:layout_constraintBottom_toTopOf="@+id/editTextCCRes"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.756">

        </EditText>

        <TextView
            android:id="@+id/textViewVote"
            android:layout_width="98dp"
            android:layout_height="36dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="88dp"
            android:gravity="center"
            android:text="Voto:"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/editTextVote"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewBirth"
            android:layout_width="98dp"
            android:layout_height="36dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="28dp"
            android:gravity="center"
            android:text="Fecha de nacimiento:"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewVote"
            app:layout_constraintEnd_toStartOf="@+id/editTextBirthRes"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewCC"
            android:layout_width="98dp"
            android:layout_height="36dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="28dp"
            android:gravity="center"
            android:text="Cedula:"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewBirth"
            app:layout_constraintEnd_toStartOf="@+id/editTextCCRes"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textViewName"
            android:layout_width="98dp"
            android:layout_height="36dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="40dp"
            android:gravity="center"
            android:text="Nombre:"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/textViewCC"
            app:layout_constraintEnd_toStartOf="@+id/editTextNameRes"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.413" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="14dp"
            android:gravity="center"
            android:text="ConfirmaciÃ³n de voto"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>